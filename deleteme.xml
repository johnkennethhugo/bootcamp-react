<?xml version="1.0"?>
<settings
    xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd"
    xmlns="http://maven.apache.org/SETTINGS/1.1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <mirrors>
        <mirror>
            <id>maven-central-mirror</id>
            <name>Internal Maven Mirror</name>
            <url>https://arti.e-bk.m086/artifactory/external-maven-org</url>
            <mirrorOf>!external,!maven-release-local,!maven-snapshot-local,!ivy-2-maven,!internal-plugin-mirror,*</mirrorOf>
        </mirror>
    </mirrors>
    <profiles>
        <profile>
            <id>p-stage-repositories</id>
            <activation>
                <?xml version="1.0"?>
                <settings
                    xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd"
                    xmlns="http://maven.apache.org/SETTINGS/1.1.0"
                    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
                    <mirrors>
                        <mirror>
                            <id>maven-central-mirror</id>
                            <name>Internal Maven Mirror</name>
                            <url>https://arti.e-bk.m086/artifactory/external-maven-org</url>
                            <mirrorOf>
                                !external,!maven-release-local,!maven-snapshot-local,!ivy-2-maven,!internal-plugin-mirror,*</mirrorOf>
                        </mirror>
                    </mirrors>
                    <profiles>
                        <profile>
                            <id>p-stage-repositories</id>
                            <activation>
                                <activeByDefault>true</activeByDefault>
                            </activation>
                            <repositories>
                                <repository>
                                    <id>external</id>
                                    <name>external</name>
                                    <url>https://arti.e-bk.m086/artifactory/external-maven-org</url>
                                </repository>
                                <repository>
                                    <snapshots>
                                        <enabled>false</enabled>
                                    </snapshots>
                                    <releases>
                                        <enabled>true</enabled>
                                    </releases>
                                    <id>maven-release-local</id>
                                    <name>maven-release-local</name>
                                    <url>https://arti.e-bk.m086/artifactory/maven-release-local</url>
                                </repository>
                                <repository>
                                    <snapshots>
                                        <enabled>true</enabled>
                                    </snapshots>
                                    <releases>
                                        <enabled>false</enabled>
                                    </releases>
                                    <id>maven-snapshot-local</id>
                                    <name>maven-snapshot-local</name>
                                    <url>https://arti.e-bk.m086/artifactory/maven-snapshot-local</url>
                                </repository>
                                <repository>
                                    <snapshots>
                                        <enabled>true</enabled>
                                    </snapshots>
                                    <releases>
                                        <enabled>true</enabled>
                                    </releases>
                                    <id>ivy-2-maven</id>
                                    <name>ivy-2-maven</name>
                                    <url>https://arti.e-bk.m086/artifactory/ivy-2-maven</url>
                                </repository>
                            </repositories>
                            <pluginRepositories>
                                <pluginRepository>
                                    <snapshots>
                                        <enabled>false</enabled>
                                    </snapshots>
                                    <releases>
                                        <enabled>true</enabled>
                                    </releases>
                                    <id>internal-plugin-mirror</id>
                                    <name>Internal Maven Plugins Mirror</name>
                                    <url>https://arti.e-bk.m086/artifactory/maven-plugin</url>
                                </pluginRepository>
                            </pluginRepositories>
                        </profile>
                    </profiles>
                </settings>
                <activeByDefault>true</activeByDefault>
            </activation>
            <repositories>
                <repository>
                    <id>external</id>
                    <name>external</name>
                    <url>https://arti.e-bk.m086/artifactory/external-maven-org</url>
                </repository>
                <repository>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <releases>
                        <enabled>
                            true</enabled>
                    </releases>
                    <id>maven-release-local</id>
                    <name>maven-release-local</name>
                    <url>https://arti.e-bk.m086/artifactory/maven-release-local</url>
                </repository>
                <repository>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                    <releases>
                        <enabled>
                            false</enabled>
                    </releases>
                    <id>maven-snapshot-local</id>
                    <name>maven-snapshot-local</name>
                    <url>https://arti.e-bk.m086/artifactory/maven-snapshot-local</url>
                </repository>
                <repository>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                    <releases>
                        <enabled>
                            true</enabled>
                    </releases>
                    <id>ivy-2-maven</id>
                    <name>ivy-2-maven</name>
                    <url>https://arti.e-bk.m086/artifactory/ivy-2-maven</url>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <id>internal-plugin-mirror</id>
                    <name>Internal Maven Plugins Mirror</name>
                    <url>https://arti.e-bk.m086/artifactory/maven-plugin</url>
                </pluginRepository>
            </pluginRepositories>
        </profile>
    </profiles>
</settings>